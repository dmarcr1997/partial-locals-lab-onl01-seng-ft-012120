<%= form_tag students_path, method: :get do |f| %>
    <label for=query> Student Name</label> 
    <%= text_field_tag :query, params[:query]%>
    <%= submit_tag "Search", name: nil%>
<%end%>

<% @students.each do |klass_student|%>
    <%=render partial: 'student', locals:{student: klass_student}%>
<%end%>

